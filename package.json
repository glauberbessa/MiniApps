{
  "name": "miniapps-launcher",
  "version": "1.0.0",
  "description": "Launcher para MiniApps - YTPlaylistManagerPro e ScanQRCodeBar",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:all": "concurrently \"npm run dev\" \"cd YTPlaylistManagerProWeb && npm run dev -- -p 3001\" \"cd ScanQRCodeBar && npm run dev -- -p 3002\"",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "postinstall": "node ./scripts/prisma-generate.mjs",
    "db:push": "prisma db push",
    "db:migrate": "node ./scripts/ensure-db.mjs",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^6.4.1",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.24.0",
    "@tanstack/react-table": "^8.12.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "googleapis": "^134.0.0",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.344.0",
    "next": "^14.2.35",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "resend": "^6.9.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^7.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "20.19.30",
    "@types/react": "18.3.27",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.0",
    "postcss": "^8.4.35",
    "prisma": "^6.4.1",
    "tailwindcss": "^3.4.1",
    "typescript": "5.9.3"
  }
}

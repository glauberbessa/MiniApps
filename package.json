{
  "name": "miniapps-launcher",
  "version": "1.0.0",
  "description": "Launcher para MiniApps - YTPlaylistManagerPro e ScanQRCodeBar",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "dev:ytpm": "cd YTPlaylistManagerProWeb && npm run dev -- -p 3001",
    "dev:scanner": "cd ScanQRCodeBar && npm run dev -- -p 3002",
    "dev:all": "npm run dev & npm run dev:ytpm & npm run dev:scanner"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  }
}
